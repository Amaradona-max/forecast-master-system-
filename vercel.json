{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api_gateway/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "api_gateway/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "dashboard/$1"
    }
  ]
}
